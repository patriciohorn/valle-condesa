---
import SectionWrapper from "../components/SectionWrapper.astro"

const { blok } = Astro.props
---

<SectionWrapper>
  <div class="md:pt-20 pt-16 pb-12 md:pb-16">
    {blok.eyebrow && <p class="mb-3 md:mb-4 text-base text-accent uppercase tracking-wider">{blok.eyebrow}</p> }
    <h2 class="text-4xl md:text-5xl md:max-w-3xl font-medium text-heading">{blok.headline}</h2>
  </div>
</SectionWrapper>


