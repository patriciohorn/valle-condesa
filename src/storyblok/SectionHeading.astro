---
import SectionWrapper from "../components/SectionWrapper.astro"

const { blok } = Astro.props
---

<SectionWrapper>
  <div class="pt-20 pb-16">
    {blok.eyebrow && <p class="mb-4 text-base text-accent uppercase tracking-wider">{blok.eyebrow}</p> }
    <h2 class="text-4xl md:text-5xl font-medium md:max-w-3xl text-heading">{blok.headline}</h2>
  </div>
</SectionWrapper>


