---
import SectionWrapper from "../components/SectionWrapper.astro"

const { blok } = Astro.props
---

<SectionWrapper>
  <div class="">
    {blok.eyebrow && <p class="mb-4 text-sm text-accent">{blok.eyebrow}</p> }
    <h2 class="mb-4 md:mb-6 text-4xl md:text-5xl">{blok.headline}</h2>
  </div>
</SectionWrapper>


